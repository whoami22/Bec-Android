<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleMenu">
        <attr name="startAngle" format="integer"/>
        <attr name="distance" format="integer"/>
        <attr name="openOnStart" format="boolean"/>
        <attr name="hintsEnabled" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CircleMenuButton">
        <attr name="colorNormal" format="color"/>
        <attr name="colorPressed" format="color"/>
        <attr name="icon" format="reference"/>
        <attr name="hintText" format="string"/>
    </declare-styleable>


</resources>