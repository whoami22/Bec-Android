<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bec.bapatla.college">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launch"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main2Activity"
            android:label="BAPATLA COLLEGE"
            android:parentActivityName=".Main2Activity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bec.bapatla.college.Main2Activity" />
        </activity>
        <activity
            android:name=".depts"
            android:label="DEPARTMENTS" />
        <activity android:name=".downloads" />
        <activity
            android:name=".sa"
            android:label="STUDENT ACTIVITIES" />
        <activity android:name=".ci" />
        <activity
            android:name=".results"
            android:label="RESULTS" />
        <activity android:name=".marks" />
        <activity android:name=".atcal" />
        <activity android:name=".midm" />
        <activity android:name=".csd" />
        <activity
            android:name=".eced"
            android:label="ECE" />
        <activity
            android:name=".eeed"
            android:label="EEE" />
        <activity
            android:name=".itd"
            android:label="IT" />
        <activity android:name=".ched" />
        <activity android:name=".mechd" />
        <activity android:name=".civd" />
        <activity android:name=".eied" />
        <activity
            android:name=".estimators"
            android:label="ESTIMATORS" />
        <activity android:name=".internalest" />
        <activity android:name=".gallery" />

        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>

            <meta-data
                android:name="com.google.firebase.messaging.default_notification_icon"
                android:resource="@drawable/logo" />
        </service>
        <service android:name=".FirebaseIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name=".notifications"
            android:label="@string/title_activity_notifications"
            android:theme="@style/AppTheme" />
        <activity android:name=".NewresultsActivity" />
        <activity android:name=".contactUs" />
        <activity android:name=".developers" />

        <meta-data
            android:name="google_analytics_adid_collection_enabled"
            android:value="false" />

        <activity android:name=".PrivacyPolicy"></activity>
    </application>

</manifest>